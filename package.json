{
	"name": "testcafe_uiautomation",
	"version": "1.0.0",
	"description": "TestCafe UI Automation framework ",
	"main": "index.js",
	"scripts": {
		"doc": "jsdoc -c jsdoc.json",
		"test:chrome": "testcafe chrome test -s takeOnFails=true",
		"test:chrome:parallel": "testcafe -c 5 chrome test -s takeOnFails=true",
		"test:chrome:headless": "testcafe chrome:headless test -s takeOnFails=true",
		"test:chrome:headless:parallel": "testcafe -c 5 chrome:headless test -s takeOnFails=true",
		"test:chrome:mobile": "testcafe chrome:emulation:device=iphone 13promax test -s takeOnFails=true",
		"test:safari": "testcafe safari test -s takeOnFails=true",
		"test:multiple": "testcafe chrome,firefox test -s takeOnFails=true",
		"test": "testcafe",
		"test:examples:chrome:headless": "testcafe chrome:headless test/testcafe-basic-examples -s takeOnFails=true --reporter html:test-artifacts/reports/reporter.html"
	},
	"keywords": [
		"TestCafe",
		"UIAutomation",
		"JavaScript"
	],
	"author": "Sritaj Kumar Patel",
	"license": "ISC",
	"dependencies": {
		"@faker-js/faker": "^6.3.1",
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"prettier": "^2.6.2",
		"testcafe": "^1.18.6",
		"testcafe-reporter-allure": "^1.0.5",
		"testcafe-reporter-html": "^1.4.6",
		"testcafe-reporter-list": "^2.1.0",
		"xpath-to-css": "^1.1.0"
	}
}
