{
	"name": "testcafe_uiautomation",
	"version": "1.0.0",
	"description": "TestCafe UI Automation framework ",
	"main": "index.js",
	"scripts": {
		"test:chrome": "testcafe chrome ./tests -s takeOnFails=true",
		"test:chrome:parallel": "testcafe -c 5 chrome ./tests -s takeOnFails=true",
		"test:chrome:headless": "testcafe chrome:headless ./tests -s takeOnFails=true",
		"test:chrome:headless:parallel": "testcafe -c 5 chrome:headless ./tests -s takeOnFails=true",
		"test:chrome:mobile": "testcafe chrome:emulation:device=iphone 13promax ./tests -s takeOnFails=true",
		"test:safari": "testcafe safari ./tests -s takeOnFails=true",
		"test:multiple": "testcafe chrome,firefox ./tests -s takeOnFails=true"
	},
	"keywords": [
		"TestCafe",
		"UIAutomation",
		"JavaScript"
	],
	"author": "Sritaj Kumar Patel",
	"license": "ISC",
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"prettier": "^2.6.2",
		"testcafe": "^1.18.6"
	},
	"devDependencies": {
		"xpath-to-css": "^1.1.0"
	}
}
